{

	"name": "test extension 1",
	"version": "0.1.1",
	"manifest_version": 2,
	"description": "this is a test",
	"content_scripts": [
		{
			"matches":[
			"https://soundcloud.com/stream*"
			],
			"js": ["content.js"]
		}	

	],
	//Browser action, essentially defines what clicking on the popup icon will do.
	"browser_action": {
	
		"default_icon": "icon.png",
		"default_popup": "mypopup.html"
	},

	"background": {
		"scripts": ["background.js"]
	}
	
	
	
}